class Solution {
    public boolean isPalindrome(int x) {
        int count=0;
        if(x<0){
            return false;
        }
        for (int i=x;i!=0;i/=10){
            count=count*10+i%10;

        }
        if(count==x){
            return true;
        }else {
            return false;
        }

    }
}
